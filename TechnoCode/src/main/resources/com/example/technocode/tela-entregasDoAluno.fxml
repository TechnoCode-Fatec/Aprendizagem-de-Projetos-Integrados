<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="TOP_CENTER" prefHeight="601.0" prefWidth="600.0" style="-fx-background-color: C4C4C4;" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.technocode.Controllers.TelaEntregasDoAluno">
    <children>
        <HBox alignment="CENTER" prefHeight="44.0" prefWidth="600.0" style="-fx-spacing: 25;">
            <children>
                <Button mnemonicParsing="false" onAction="#voltarTelaOrientador" prefHeight="30.0" prefWidth="113.0" style="-fx-background-color: #5E5555;" text="Voltar" textFill="WHITE">
                    <font>
                        <Font size="14.0" />
                    </font>
                </Button>
                <Label fx:id="labelTitulo" alignment="CENTER" prefHeight="47.0" prefWidth="295.0" text="ANÁLISE DO ALUNO">
                    <font>
                        <Font name="System Bold" size="26.0" />
                    </font>
                </Label>
            </children>
        </HBox>
        <VBox alignment="CENTER" prefHeight="215.0" prefWidth="600.0" style="-fx-spacing: 15; -fx-padding: 20;">
            <children>
                <VBox prefHeight="115.0" prefWidth="560.0" style="-fx-background-color: EAEAEA; -fx-padding: 20;">
                    <children>
                        <Label text="Informações do Aluno">
                            <font>
                                <Font name="System Bold" size="18.0" />
                            </font>
                        </Label>
                        <VBox prefHeight="94.0" prefWidth="520.0" style="-fx-spacing: 10; -fx-padding: 10;">
                            <children>
                                <Label fx:id="nomeAluno" text="Nome: ">
                                    <font>
                                        <Font size="14.0" />
                                    </font>
                                </Label>
                                <Label fx:id="emailAluno" text="Email: ">
                                    <font>
                                        <Font size="14.0" />
                                    </font>
                                </Label>
                                <Label fx:id="cursoAluno" text="Matriculado em: ">
                                    <font>
                                        <Font size="14.0" />
                                    </font>
                                </Label>
                            </children>
                        </VBox>
                    </children>
                </VBox>
            </children>
        </VBox>
      <TableView fx:id="tabelaSecao" prefHeight="316.0" prefWidth="600.0">
        <columns>
            <TableColumn fx:id="colNomeSecao" prefWidth="72.20001375675201" text="Nome" />
            <TableColumn fx:id="colDescricao" minWidth="0.0" prefWidth="78.1999740600586" text="Descrição" />
            <TableColumn fx:id="colAnalisar" prefWidth="120.0" text="Analisar Seção" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
    </children>
</VBox>
