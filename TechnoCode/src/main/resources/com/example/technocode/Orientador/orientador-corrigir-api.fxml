                                                <TextArea fx:id="feedbackProblema" prefHeight="80.0" promptText="Digite seu feedback..." style="-fx-background-color: #FFFFFF; -fx-border-color: #E0E0E0; -fx-border-radius: 6; -fx-background-radius: 6; -fx-padding: 10; -fx-wrap-text: true; -fx-text-fill: #2C3E50;" wrapText="true" />
                                            </VBox>
                                        </VBox>

                                        <!-- Solução -->
