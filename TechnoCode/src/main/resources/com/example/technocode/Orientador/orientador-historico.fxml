<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="TOP_CENTER" prefHeight="700.0" prefWidth="800.0" style="-fx-background-color: C4C4C4;" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.technocode.Controllers.Orientador.OrientadorHistoricoController">
    <children>
        <HBox alignment="CENTER" prefHeight="60.0" prefWidth="800.0" style="-fx-spacing: 25; -fx-padding: 10 20 10 20;">
            <children>
                <Button mnemonicParsing="false" onAction="#voltarTelaEntregas" prefHeight="34.0" prefWidth="130.0" style="-fx-background-color: B82E1A;" text="Voltar" textFill="WHITE">
                    <font>
                        <Font size="14.0" />
                    </font>
                </Button>
                <Label fx:id="labelTitulo" alignment="CENTER" prefHeight="47.0" prefWidth="400.0" text="HISTÓRICO DE VERSÕES">
                    <font>
                        <Font name="System Bold" size="24.0" />
                    </font>
                </Label>
            </children>
        </HBox>

        <ScrollPane fitToWidth="true" prefHeight="560.0" prefWidth="780.0" style="-fx-background-color: transparent;" vbarPolicy="ALWAYS">
            <content>
                <VBox alignment="TOP_CENTER" prefWidth="760.0" style="-fx-spacing: 15; -fx-padding: 20;">
                    <children>
                        <!-- Container para apresentações -->
                        <VBox fx:id="containerApresentacoes" alignment="TOP_CENTER" prefWidth="720.0" style="-fx-spacing: 10;">
                            <children>
                                <Label style="-fx-font-weight: bold; -fx-font-size: 18px; -fx-text-fill: #333;" text="APRESENTAÇÕES">
                                    <font>
                                        <Font name="System Bold" size="18.0" />
                                    </font>
                                </Label>
                            </children>
                        </VBox>
                        
                        <!-- Container para APIs -->
                        <VBox fx:id="containerApis" alignment="TOP_CENTER" prefWidth="720.0" style="-fx-spacing: 10;">
                            <children>
                                <Label style="-fx-font-weight: bold; -fx-font-size: 18px; -fx-text-fill: #333;" text="APIs">
                                    <font>
                                        <Font name="System Bold" size="18.0" />
                                    </font>
                                </Label>
                            </children>
                        </VBox>
                    </children>
                </VBox>
            </content>
        </ScrollPane>
    </children>
</VBox>
